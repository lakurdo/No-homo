<Page x:Class="ЧЗКзаеб.E2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ЧЗКзаеб"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="E2" Background="Lavender">

    <Grid>
        <StackPanel Grid.Row="1" VerticalAlignment="Center">
            <Button x:Name="trrrr" Style="{StaticResource trrrr}" Content="нажми сюда, долбаеб" Margin="10" Click="trrrr_Click" Width="500" />
            <Button x:Name="hehehe" Style="{StaticResource hehehe}" Content="попробуй нажми, уебок" Click="hehehe_Click" Margin="10" Width="500" />
            <Button x:Name="ventilator" Style="{StaticResource ventilator}" Content="мышку наведи, гандон" Click="ventilator_Click" Margin="10" Width="500" />
        </StackPanel>

    </Grid>
</Page>
